{
  "name": "hani-editor",
  "version": "0.0.5",
  "description": "Haniplanet Editor with Renderer",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn build:clean && yarn build:hani-editor && yarn build:editor-lib",
    "build:clean": "rm -rf build",
    "build:hani-editor": "parcel build src/index.ts --out-dir build --global hani-editor",
    "build:editor-lib": "parcel build src/lib/index.ts --out-dir build/lib --global editor-lib"
  },
  "repository": "https://github.com/haniplanet/atlaskit-custom-editor.git",
  "author": "Im Yongbeen <meis1541@naver.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@atlaskit/editor-core": "^107.24.3",
    "@atlaskit/editor-json-transformer": "^5.0.2",
    "@atlaskit/media-core": "^29.1.5",
    "@atlaskit/renderer": "^45.6.2",
    "lodash.isequal": "^4.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "2.6.0",
    "styled-components": "3.2.6"
  }
}
