{
  "name": "atlaskit-custom-editor",
  "version": "0.0.1",
  "description": "Atlaskit Editor with Custom Extensions",
  "main": "build/index.js",
  "author": "jungpaeng",
  "license": "MIT",
  "scripts": {
    "build": "yarn rm:build && yarn build:ts && yarn build:webpack && yarn git",
    "rm:build": "rm -rf build/",
    "build:ts": "tsc",
    "build:webpack": "WEBPACK_ENV=production webpack",
    "git": "yarn git:add && yarn git:checkout:build && yarn git:commit && yarn git:push && yarn git:reset && yarn git:checkout:master",
    "git:add": "git add -f package.json build/",
    "git:checkout:build": "git checkout build",
    "git:checkout:master": "git checkout master",
    "git:commit": "git commit -m \"Build Package\"",
    "git:push": "git push --force origin master:build",
    "git:reset": "git reset HEAD^"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "^16.8.16",
    "babel-loader": "^8.0.5",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
