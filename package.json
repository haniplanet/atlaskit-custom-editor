{
  "name": "atlaskit-custom-editor",
  "version": "0.0.1",
  "description": "Atlaskit Editor with Custom Extensions",
  "main": "build/index.js",
  "author": "jungpaeng",
  "license": "MIT",
  "scripts": {
    "build": "yarn rm:build && yarn build:ts && yarn build:webpack && yarn git",
    "rm:build": "rm -rf build/",
    "build:ts": "tsc",
    "build:webpack": "WEBPACK_ENV=production webpack",
    "git": "yarn git:add:master && yarn git:build && yarn git:co:master && git reset HEAD^1",
    "git:build": "yarn git:co:build && yarn git:clear:build && yarn git:add:build && git push",
    "git:co:master": "git checkout master",
    "git:co:build": "git checkout build",
    "git:add:master": "git add -f package.json build/ && git commit -m \"Build Package\"",
    "git:clear:build": "rm -rf build && git add -f build/ && git commit -m \"Delete Build\"",
    "git:add:build": "git cherry-pick master"
  },
  "dependencies": {
    "@atlaskit/editor-common": "^39.7.0",
    "@atlaskit/editor-core": "^112.11.8",
    "@atlaskit/media-core": "^30.0.4",
    "@atlaskit/renderer": "^48.7.1",
    "@babel/runtime": "^7.4.4",
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "styled-jsx": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.16",
    "@types/styled-jsx": "^2.2.8",
    "babel-loader": "^8.0.5",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
